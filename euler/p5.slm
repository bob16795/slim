inc stdlib.slm

gvar temp 8
gvar oldtemp 8

const max 20

proc main 0 0
  temp 1 put disc
  oldtemp 1 put disc
  1
  do
    1 +
    
    copy temp read swap / swap disc 0 != if [
      do
        temp
        temp read oldtemp read +
        put disc

        copy temp read swap / swap disc 0 !=
      end
    ]
    oldtemp temp read put disc
    
    copy max <
  end
  disc
  
  temp read (int.print)

  ret
end
