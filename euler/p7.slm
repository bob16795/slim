inc stdlib.slm

gvar testing 8

proc checkprime 1 1
  testing swap put disc
  1
  do
    1 +
    testing read covr / swap disc 0 == if [disc 0 ret]

    copy copy * testing read <
  end
  disc
  1

  ret
end

proc main 0 0
  10000
  1
  do
    1 +

    copy (checkprime) if [
      swap 1 -
      swap
    ]

    covr 0 !=
  end
  int.print
  disc

  ret
end
