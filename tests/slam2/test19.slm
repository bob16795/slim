inc "stdlib.slm"
inc "stdlib/testin.slm"

proc runs 0 0
  
    var lmao 8

  "set it to 0" (testin.next)

    lmao 10 put disc

  "get the value" (testin.next)

    lmao read 10 (testin.eq) 

  ret
end

proc main 0 0
  "Create a lvar" (testin.setup)
  
    0
    runs
    runs

    0 (testin.eq)
    
  (testin.done)
  
  0 quit

  ret
end
