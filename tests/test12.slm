inc stdlib.slm
inc testin.slm

var temp 1

oper proc foo 0 0
  temp 1 putc disc
  
  ret
end

oper proc add 2 1
  +
  
  ret
end

proc main 0 0
  "Call a oper function that takes nothing" (testin.setup)
  
    foo

    temp read (testin.true)
  
  "Call a oper function that takes stuff" (testin.next)

    1 1 add
    2 (testin.eq)

  (testin.done)

  ret
end
